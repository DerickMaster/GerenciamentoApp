<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_pressed="true" >
    <shape android:shape="rectangle"  >
      <corners android:radius="12dip" />
      <stroke android:width="1dip" android:color="#1e9e73" />
      <gradient android:angle="-90" android:startColor="#3cd6a2" android:endColor="#3cd6a2"  />
    </shape>
  </item>
  <item android:state_focused="true">
    <shape android:shape="rectangle"  >
      <corners android:radius="12dip" />
      <stroke android:width="1dip" android:color="#31ad54" />
      <solid android:color="#31ad54"/>
    </shape>
  </item>
  <item >
    <shape android:shape="rectangle"  >
      <corners android:radius="12dip" />
      <stroke android:width="1dip" android:color="#31ad54" />
      <gradient android:angle="-90" android:startColor="#31ad54" android:endColor="#31ad54" />
    </shape>
  </item>
</selector>